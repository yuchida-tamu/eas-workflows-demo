{
	"cli": {
		"version": ">= 15.0.10",
		"appVersionSource": "remote"
	},
	"build": {
		"development": {
			"withoutCredentials": true,
			"developmentClient": true,
			"distribution": "internal",
			"channel": "development",
			"ios": {
				"simulator": true,
				"image": "latest"
			}
		},
		"preview": {
			"distribution": "internal",
			"channel": "preview",
			"ios": {
				"simulator": true,
				"image": "latest"
			}
		},
		"production": {
			"autoIncrement": true,
			"channel": "production"
		}
	},
	"submit": {
		"production": {}
	}
}
